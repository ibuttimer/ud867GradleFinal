<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ie.ianbuttimer.androidjoker.LibraryActivity">

    <TextView
        android:id="@+id/tv_joke_libraryActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/joke_margin"
        android:layout_marginEnd="@dimen/joke_margin"
        android:layout_marginRight="@dimen/joke_margin"
        android:layout_marginStart="@dimen/joke_margin"
        android:layout_marginLeft="@dimen/joke_margin"
        android:layout_marginTop="@dimen/joke_margin"
        tools:text="@string/joke_text"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>
